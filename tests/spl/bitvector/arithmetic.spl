procedure main() returns (res: Int) {
  var a := 23;
  var b := a + 1;
  pure assert b == 24;
  var c := (1024 + 23) & 0xFF;
  pure assert c == 23;
  var d := int2byte(c);
  pure assert d == int2byte(23);
  pure assert d >= int2byte(23);
  pure assert d <= int2byte(23);
  pure assert c == byte2int(d);
  var e := d & int2byte(0xF);
  pure assert e == int2byte(7);
  return 0;
}
